<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nix.good.dao.ContractMapper">
    <resultMap id="BaseResultMap" type="com.nix.good.model.ContractModel">
        <id column="id" property="id" jdbcType="INTEGER"/>
        <result property="contractId" column="contractId" jdbcType="VARCHAR"/>
        <result property="createDate" column="createDate" jdbcType="TIMESTAMP"/>
        <association property="customer" javaType="com.nix.good.model.MemberModel">
            <result property="memberId" column="memberId" jdbcType="VARCHAR"/>
            <result property="password" column="password" jdbcType="VARCHAR"/>
            <result property="name" column="name" jdbcType="VARCHAR"/>
            <result property="role" column="role" jdbcType="INTEGER"/>
            <result property="sex" column="sex" jdbcType="BIT"/>
            <result property="age" column="age" jdbcType="INTEGER"/>
        </association>
        <association property="admin" column="admin" javaType="com.nix.good.model.MemberModel">
            <result property="memberId" column="memberId" jdbcType="VARCHAR"/>
            <result property="password" column="password" jdbcType="VARCHAR"/>
            <result property="name" column="name" jdbcType="VARCHAR"/>
            <result property="role" column="role" jdbcType="INTEGER"/>
            <result property="sex" column="sex" jdbcType="BIT"/>
            <result property="age" column="age" jdbcType="INTEGER"/>
        </association>
    </resultMap>

    <select id="select" parameterType="int" resultMap="BaseResultMap">
        select * from `contract` as c where c.id = #{id,jdbcType=INTEGER}
    </select>
</mapper>